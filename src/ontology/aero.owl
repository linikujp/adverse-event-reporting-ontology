    <!-- http://purl.obolibrary.org/obo/AERO_0000021 -->

    <owl:Class rdf:about="&obo;AERO_0000021">
        <rdfs:label xml:lang="en">seizure level 1 of certainty</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;AERO_0000017"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&ro;has_part"/>
                <owl:someValuesFrom rdf:resource="&obo;AERO_0000048"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&obo;OBI_0000308"/>
                <owl:someValuesFrom>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&obo;OBI_0000294"/>
                        <owl:someValuesFrom rdf:resource="&obo;AERO_0000003"/>
                    </owl:Restriction>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&ro;has_part"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="&obo;AERO_0000026"/>
                            <rdf:Description rdf:about="&obo;AERO_0000029"/>
                            <rdf:Description rdf:about="&obo;AERO_0000030"/>
                            <rdf:Description rdf:about="&obo;AERO_0000031"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
