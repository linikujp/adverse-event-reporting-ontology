    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/AERO_0000110 -->

    <owl:Class rdf:about="&obo;AERO_0000110">
        <rdfs:label xml:lang="en">tachycardia</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;AERO_0000211"/>
        <obo:AERO_0000324 xml:lang="en">A heart rate that is abnormally high for age and circumstance (In beats per minute: &lt;1year old: &gt;160; 1 – 2 yrs: &gt;150; 2-5 yrs: &gt;140; 5-12 yrs: &gt;120; &gt;12 yrs: &gt;100)</obo:AERO_0000324>
        <obo:IAO_0000119 xml:lang="en">Citation for norms is Don &amp; Roberton; Physical examination; in Practical Pediatrics 2007; 6th ed</obo:IAO_0000119>
        <obo:AERO_0000106>TacchyCard</obo:AERO_0000106>
        <obo:IAO_0000119 xml:lang="en">Vaccine 28 (2010) 4487-4498</obo:IAO_0000119>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/AERO_0000111 -->

    <owl:Class rdf:about="&obo;AERO_0000111">
        <rdfs:label xml:lang="en">capillary refill time &gt; 3s</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;AERO_0000211"/>
        <obo:AERO_0000106>CapRefillOver3</obo:AERO_0000106>
        <obo:AERO_0000324 xml:lang="en">Capillary refill time is the time required for the normal skin colour to reappear after a blanching pressure is applied. It is usually performed by pressing on the nail bed to cause blanching and then counting the time it takes for the blood to return to the tissue, indicated by a pink colour returning to the nail. Normally it is &lt;3 seconds.</obo:AERO_0000324>
        <obo:IAO_0000119 xml:lang="en">Vaccine 28 (2010) 4487-4498</obo:IAO_0000119>
    </owl:Class>
    


